/* pages/orderlist/orderlist.wxss */
.page-box{
  /* 生成固定定位的元素，相对于浏览器窗口进行定位。 */
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgb(224, 222, 222);
  display: flex;
  flex-direction: column;
}
.page-content-box{
  width: 100%;
  height: 100%;
}
.page-order-list-item{
  height: 100px;
  margin: 10px;
  border-radius: 10px;
  background: lightseagreen;
}
